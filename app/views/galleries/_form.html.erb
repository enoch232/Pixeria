<div class= "default-page">
<%= form_for (@gallery), html: {multipart: true} do |f| %>
	<br>
	<%= f.label :title %>
	<br>
	<%= f.text_field :title %>
	<br>
	<%= f.label :description %>
	<br>
	<%= f.text_field :description %>
	<br>
	<%= f.label :images %>
	<br>
	<%= file_field_tag "images[]", type: :file, multiple: :true %>
	<br>
	<%= f.submit class: "btn btn-default" %>
<% end %>
</div>


